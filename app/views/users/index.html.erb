<% @users.each do |user| %>
  <td><%= user.username %></td>
  <td><%= user.email %></td>
  <td><%= link_to 'User page', user_path(user) %></td>
  <td><%= link_to 'Edit', edit_user_path(user) %></td>
  <td><%= link_to 'Destroy', user_path(user),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
